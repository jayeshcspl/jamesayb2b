<!doctype html>
<html class="no-js supports-no-cookies" lang="{{ shop.locale }}">
<head>
  {% include '3wh-gtm' %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0,user-scalable=0"/>
  <meta name="theme-color" content="{{ settings.color_accent }}">
  <meta http-equiv="Expires" content="30" />
  <link rel="canonical" href="{{ canonical_url }}">
  {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {%- endif -%}

  {%- capture seo_title -%}
    {{ page_title }}
    {%- if current_tags -%}
      {%- assign meta_tags = current_tags | join: ', ' -%} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {%- endif -%}
    {%- if current_page != 1 -%}
      &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {%- endif -%}
    {%- assign escaped_page_title = page_title | escape -%}
    {%- unless escaped_page_title contains shop.name -%}
      &ndash; {{ shop.name }}
    {%- endunless -%}
  {%- endcapture -%}
  <title>{{ seo_title | strip }}</title>

  {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}">
  {%- endif -%}

  {% include 'social-meta-tags' %}
  {% include 'style-tags', layout: 'theme' %}
  {% include 'css-variables' %}
  {% include 'script-tags', layout: 'theme' %}
  {% if request.host != 'wholesale-jamesay.myshopify.com' %}
    {{ content_for_header }}
  {% endif %}
  <link defer rel="stylesheet" type="text/css" href="{{ 'fontawesome_v5.8.2_all.css' | asset_url }}">
  <!-- {% if template == 'product' %}
  <link defer rel="stylesheet" type="text/css" href="{{ 'jquery.fancybox.min.css' | asset_url }}">
  {% endif %} -->
  {% if template == 'index' or template == 'blog' %}
  <link defer rel="stylesheet" type="text/css" href="{{ 'jquery.fullPage.css' | asset_url }}">
  {% endif %}
  <link defer rel="stylesheet" type="text/css" href="{{ 'jquery.bxslider.min.css' | asset_url }}">
  <link defer rel="stylesheet" type="text/css" href="{{ 'animate.min.css' | asset_url }}">
  <link defer rel="stylesheet" type="text/css" href="{{ 'aos.css' | asset_url }}">
  <link defer rel="stylesheet" type="text/css" href="{{ 'style.scss' | asset_url }}">
  <link defer rel="stylesheet" type="text/css" href="{{ 'responsive.css' | asset_url }}">
  <link defer rel="stylesheet" type="text/css" href="{{ 'select-css.css' | asset_url }}">
  <link defer rel="stylesheet" type="text/css" href="{{ 'product_rollover.css' | asset_url }}">
  <style>

    @media screen and (min-width: 992px) {
      body:after{
        display:none;
        content: {% if template == 'index' %} url({{ 'videobg_vimeo.jpg' | file_url }}) {% endif %}
        url({{ '01.jpg' | file_url }})
        url({{ '02.jpg' | file_url }})
        url({{ '03.jpg' | file_url }})
        url({{ '04.jpg' | file_url }});
      }
      #pic {
        opacity: 1;
        transition: opacity .25s ease-in-out;
        -moz-transition: opacity .25s ease-in-out;
        -webkit-transition: opacity .25s ease-in-out;
      }
      #pic.p1 {
        background-image: url({{ '01.jpg' | file_url }});
        background-size: cover;
        background-position: center center;
      }
      #pic.p2 {
        background-image: url({{ '02.jpg' | file_url }});
        background-size: cover;
        background-position: center center;
      }
      #pic.p3 {
        background-image: url({{ '03.jpg' | file_url }});
        background-size: cover;
        background-position: center center;
      }
      #pic.p4 {
        background-image: url({{ '04.jpg' | file_url }});
        background-size: cover;
        background-position: center center;
      }
    }
    @media screen and (max-width: 991px) {
      {% if template == 'index' %}
      body:after{
        display:none;
        content: url({{ 'videobg_vimeo_mobile.jpg' | file_url }});
      }
      {% endif %}
    }    
    a.product_link,
    a.product_link:hover {
      color: inherit;
    }
    .james, .ays {
      transition: all 0.5s;
    }
    .ays {
      display: none;
    }
    @media screen and (min-width: 992px) {
      .vimeo-wrapper {
        background-image: url({{ 'videobg_vimeo.jpg' | file_url }});
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
      }
    }
    @media screen and (max-width: 991px) {
      .vimeo-wrapper {
        background-image: url({{ 'videobg_vimeo_mobile.jpg' | file_url }});
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;        
      }
    }
  </style>
  <script src="{{ 'jquery-3.4.1.min.js' | asset_url }}"></script>
  <!-- {% if template == 'product' %}
  <script defer src="{{ 'jquery.fancybox.js' | asset_url }}"></script>
  {% endif %} -->
  {% if template == 'index' or template == 'blog' %}
  <script defer src="{{ 'jquery.fullPage.min.js' | asset_url }}"></script>
  {% endif %}
  <script defer src="{{ 'jquery.bxslider.min.js' | asset_url }}"></script>
  <script defer src="{{ 'highlight.min.js' | asset_url }}"></script>
  <script defer src="{{ 'aos.js' | asset_url }}"></script>
  {% if template == 'index' or template == 'article' %}
  <script src="{{ 'player.js' | asset_url }}"></script>
  {% endif %}
  <script type="text/javascript" src="//widget.trustpilot.com/bootstrap/v5/tp.widget.bootstrap.min.js" async></script>
  <!-- {{ 'luxy.js' | asset_url | script_tag }} -->
  <script defer src="{{ 'index.js' | asset_url }}"></script>
  <script>
    document.documentElement.className = document.documentElement.className.replace('no-js', '');
    window.theme = {
      strings: {
        addToCart: {{ 'products.product.add_to_cart' | t | json }},
        soldOut: {{ 'products.product.sold_out' | t | json }},
        unavailable: {{ 'products.product.unavailable' | t | json }}
      },
      moneyFormat: {{ shop.money_format | json }}
    };
    
    {% if template.directory == 'customers' %}
    theme.allCountryOptionTags = {{ all_country_option_tags | json }};
    {% endif %}
  </script>
</head>

<body id="{{ page_title | handle }}" class="template-{{ template.name | handle }}">
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-P63MCKP"
  height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
