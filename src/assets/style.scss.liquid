@font-face {
	font-family: 'MonumentRegular';
	src: url('./MonumentGroteskRegular.eot');
	src: url('./MonumentGroteskRegular.eot?#iefix') format('embedded-opentype'), 
	url('./MonumentGroteskRegular.woff2') format('woff2'), 
	url('./MonumentGroteskRegular.woff') format('woff'), 
	url('./MonumentGroteskRegular.ttf') format('truetype'), 
	url('./MonumentGroteskRegular.svg#MonumentRegular') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'MonumentSemiMono';
	src: url('./MonumentGroteskSemiMono.eot');
	src: url('./MonumentGroteskSemiMono.eot?#iefix') format('embedded-opentype'), 
	url('./MonumentGroteskSemiMono.woff2') format('woff2'), 
	url('./MonumentGroteskSemiMono.woff') format('woff'), 
	url('./MonumentGroteskSemiMono.ttf') format('truetype'), 
	url('./MonumentGroteskSemiMono.svg#MonumentSemiMono') format('svg');
	font-weight: normal;
	font-style: normal;
}
